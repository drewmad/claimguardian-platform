{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:types": "supabase gen types typescript --linked --schema public,claims,properties > ../../packages/db/src/types/database.types.ts",
    "db:types:local": "supabase gen types typescript --local --schema public,claims,properties > ../../packages/db/src/types/database.types.ts",
    "db:diff": "supabase db diff --linked",
    "db:lint": "supabase db lint --local --fail-on warning",
    "db:dump": "supabase db dump --linked -f ../../supabase/schema.sql --keep-comments",
    "db:push:dry": "supabase db push --dry-run",
    "db:reset:safe": "pnpm db:dump && supabase db reset --local",
    "db:validate": "pnpm db:lint && pnpm db:types:local",
    "db:status": "supabase db remote status",
    "db:pull": "supabase db pull",
    "db:backup": "../../scripts/db-backup.sh",
    "db:backup:all": "../../scripts/db-backup.sh both --with-data",
    "pre-migration": "pnpm db:backup && pnpm db:validate"
  },
  "dependencies": {
    "@claimguardian/ui": "workspace:*",
    "@claimguardian/utils": "workspace:*",
    "@eslint/eslintrc": "^3.3.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.50.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/dompurify": "^3.2.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/validator": "^13.12.2",
    "@zxing/library": "^0.21.3",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "sonner": "^2.0.6",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "ua-parser-js": "^2.0.4",
    "validator": "^13.15.15",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@claimguardian/config": "workspace:*",
    "@sentry/nextjs": "^7.x.x",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint-config-next": "15.3.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3"
  }
}