{
  "name": "@claimguardian/etl",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "ClaimGuardian Florida ETL service with Great Expectations",
  "engines": {
    "node": ">=20.10.0",
    "python": ">=3.11.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsx build src/index.ts --outdir=dist",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "expectations:run": "python -m great_expectations checkpoint run all",
    "expectations:docs": "python -m great_expectations docs build"
  },
  "dependencies": {
    "@temporalio/activity": "^1.10.3",
    "@temporalio/worker": "^1.10.3",
    "@temporalio/common": "^1.10.3",
    "pg": "^8.11.3",
    "ioredis": "^5.4.1",
    "pino": "^8.17.2",
    "zod": "^3.23.8",
    "csv-parse": "^5.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.0",
    "tsx": "^4.7.1",
    "eslint": "^9.5.0",
    "typescript": "^5.5.4",
    "vitest": "^1.3.1"
  }
}